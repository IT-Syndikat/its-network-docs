config wifi-device  radio0
	option type     mac80211
	option channel  1
	option hwmode	11g
	option path	'platform/qca953x_wmac'
	option htmode	HT20
	option disabled '1'

config wifi-iface
	option device   radio0
	option network  lan
	option mode     ap
	option ssid     it-syndikat
	option encryption psk2
	option key      '<WIRELESS KEY HERE>'
